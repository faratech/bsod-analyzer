{
  "name": "bsod-minidump-analyzer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "node server.js",
    "dev:frontend": "vite",
    "build": "vite build && node scripts/generate-sri.js",
    "build:no-sri": "vite build",
    "preview": "vite preview",
    "start": "NODE_ENV=production node server.js",
    "analyze-css": "node scripts/purge-css.js",
    "optimize-css": "node scripts/purge-css.js --apply"
  },
  "dependencies": {
    "@google/generative-ai": "latest",
    "compression": "latest",
    "concurrently": "latest",
    "cookie-parser": "latest",
    "cors": "latest",
    "dotenv": "latest",
    "express": "latest",
    "express-rate-limit": "latest",
    "form-data": "^4.0.5",
    "jszip": "latest",
    "multer": "^2.0.2",
    "react": "latest",
    "react-dom": "latest",
    "react-markdown": "latest",
    "react-router-dom": "^7.12.0",
    "remark-gfm": "latest",
    "xxhash-wasm": "latest"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "latest",
    "@tailwindcss/postcss": "latest",
    "@types/node": "^25.0.6",
    "@types/react": "^19.2.8",
    "@types/react-dom": "latest",
    "autoprefixer": "latest",
    "postcss": "latest",
    "purgecss": "latest",
    "tailwindcss": "latest",
    "terser": "latest",
    "typescript": "latest",
    "vite": "^7.3.1"
  }
}
